{
    "name": "friendsofcake/authenticate",
    "type": "cakephp-plugin",
    "description": "CakePHP plugin with authentication classes for AuthComponent.",
    "keywords":[
        "cakephp",
        "authenticate"
    ],
    "homepage": "http://github.com/FriendsOfCake/Authenticate",
    "authors":[
        {
            "name":"Ceeram",
            "role":"Author"
        },
        {
            "name":"ADmad",
            "role":"Developer",
            "homepage":"https://github.com/ADmad"
        }
    ],
    "license": "MIT",
    "support":{
        "source":"https://github.com/FriendsOfCake/Authenticate",
        "issues":"https://github.com/FriendsOfCake/Authenticate/issues",
        "irc":"irc://irc.freenode.org/friendsofcake"
    },
    "require": {
        "php": ">=5.4.0",
        "cakephp/cakephp": "3.0.*-dev",
        "cakephp/plugin-installer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "FOC\\Authenticate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FOC\\Authenticate\\Test\\": "tests"
        }
    }
}
